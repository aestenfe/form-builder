{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "extends": "airbnb",
  "env": {
      "browser": true
  },
  "rules": {
      "semi": "error",
      "quotes": ["error", "double"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}